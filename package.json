{
  "name": "my-qwik-site-with-visual-cms",
  "description": "Site integrated with Builder.io for visual editing",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "netlify deploy --build",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/dev-tools": "^1.0.17",
    "@builder.io/partytown": "^0.10.2",
    "@builder.io/qwik": "^1.5.7",
    "@builder.io/qwik-city": "^1.5.7",
    "@builder.io/sdk-qwik": "^0.14.26",
    "@csstools/postcss-light-dark-function": "^1.0.5",
    "@netlify/edge-functions": "^2.8.1",
    "@storybook/addon-essentials": "^8.1.10",
    "@storybook/addon-links": "^8.1.10",
    "@storybook/blocks": "^8.1.10",
    "@storybook/builder-vite": "^8.1.10",
    "@storybook/html": "^8.1.10",
    "@storybook/html-vite": "^8.1.10",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@typescript-eslint/utils": "^7.13.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.5.7",
    "netlify-cli": "^17.29.0",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.5.14",
    "prettier": "^3.3.2",
    "storybook": "^8.1.10",
    "storybook-framework-qwik": "^0.4.0",
    "typescript": "^5.5.2",
    "undici": "*",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "css-has-pseudo": "^6.0.5",
    "open-props": "^1.7.4"
  }
}
