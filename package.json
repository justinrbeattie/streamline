{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/netlify-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "netlify deploy --build",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/dev-tools": "^0.2.19",
    "@builder.io/partytown": "^0.9.2",
    "@builder.io/qwik": "^1.4.5",
    "@builder.io/qwik-city": "^1.4.5",
    "@builder.io/sdk-qwik": "^0.14.4",
    "@csstools/postcss-oklab-function": "^3.0.10",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/builder-vite": "^7.6.17",
    "@storybook/html": "^7.6.17",
    "@storybook/html-vite": "^7.6.17",
    "@types/eslint": "^8.56.4",
    "@types/node": "^20.11.21",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "10.4.17",
    "css-has-pseudo": "^6.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.4.5",
    "netlify-cli": "^17.17.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "storybook": "^7.6.17",
    "storybook-framework-qwik": "^0.3.0",
    "typescript": "5.3.3",
    "undici": "^6.6.2",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "dependencies": {
    "@qwikest/icons": "^0.0.13",
    "open-props": "^1.6.20",
    "postcss-preset-env": "^9.4.0"
  }
}
